@model TrashCollector.Models.ApplicationUser
@{
    ViewBag.Title = "WorkingZipcod";
}
<h2>Working Zipcode</h2>

@using (Html.BeginForm("EditZip", "Home", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{ 

<div class="form-group">
    @Html.LabelFor(m => m.Workingzipcode, new { @class = "col-md-2 control-label" })
    <div class="col-md-10">
        @Html.TextBoxFor(m => m.Workingzipcode, new { @class = "form-control" })
    </div>
</div>
<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <input type="submit" class="btn btn-default" value="Reset" />
    </div>
</div>
}

